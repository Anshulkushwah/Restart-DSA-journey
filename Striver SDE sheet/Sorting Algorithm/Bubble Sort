void bubbleSort(vector<int>& arr, int n) 
{
    //write your code here
        for (int i = 0; i < n - 1; i++){
		    for (int j = 0; j < n - i - 1; j++){
		    	// If jth element is greater than 'j + 1' th element
		    	// swap them
		    	if (arr[j] > arr[j + 1])   
                    swap(arr[j],arr[j + 1]);
		    }
	}
}
