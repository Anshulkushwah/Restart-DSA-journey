int countDigits(int n){
	// Write your code here.

	int count = 0, digit,Num=n;

        while (Num >0) {
          digit = Num % 10;
		  cout<<digit << " = " <<Num%10; <<endl;
          if (digit != 0 && n % digit == 0 ){
            count++;
        }

                Num /=10;
				count<<" after count " << Num <<endl;
	}


return  count;
}
