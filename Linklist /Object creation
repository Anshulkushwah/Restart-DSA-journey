#include <iostream>

using namespace std;

class Node {
    // data member are below     
    public:
    int data;
    Node* next;
    
    // constructor
    Node (int d){
        this->data = d;
        this->next = NULL;
    }
    
    
};

int main()
{
    //  object creation
    Node* first = new Node(3);
    cout << first -> data << endl ;
    cout << first -> next << endl ;

    return 0;
}
